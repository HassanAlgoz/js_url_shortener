{
  "name": "url_shortener",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tsup",
    "start:dev": "node --import=tsx --watch src/index.ts",
    "test": "vitest run",
    "test:cov": "vitest run --coverage"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "express": "^5.1.0",
    "shortid": "^2.2.17"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "outDir": "dist",
    "format": [
      "esm",
      "cjs"
    ],
    "target": "es2020",
    "sourcemap": true,
    "clean": true,
    "dts": true,
    "splitting": false,
    "skipNodeModulesBundle": true
  },
  "devDependencies": {
    "@types/shortid": "^2.2.0",
    "@types/express": "^5.0.3",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "supertest": "^7.1.4",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
